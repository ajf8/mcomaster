<p style="margin-top:20px;margin-bottom:20px;"><strong>Result from {{senderid}}</strong></p>
<table class="table table-striped">
<thead>
	<tr><th>Output</th><th>Value</th></tr>
</thead>
<tbody>
<tr><td>Status</td>
<td>
{{#if badgeClass}}
<span class="badge badge-{{badgeClass}}">
{{/if}}
{{body.statusmsg}}
{{#if badgeClass}}
</span>
{{/if}}
</td>
</tr>
{{#each rows}}
<tr><td>{{kee}}</td><td>
{{#if isComplex}}
<div class="complex" data-column="{{kee}}"></div>
{{else}}
{{nl2br val}}
{{/if}}
</td>
</tr>
{{/each}}
</tbody>
</table>